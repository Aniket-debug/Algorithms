#include <iostream>
#include<bits/stdc++.h>
using namespace std;

void Bellman_Ford_Algo(vector<pair<int,int>>& graph,int vertices,int edges){
    vector<int> dis(vertices,INT_MAX);
    dis[0]=0;
    
}

int main()
{
    int vertices,edges;
    cin>>vertices>>edges;
    vector<pair<int,int>> graph[vertices];
    for(int i=0;i<edges;i++){
        int x,y,z;
        cin>>x>>y>>z;
        graph[x].push_back({y,z});
    }
    Bellman_Ford_Algo(*graph,vertices,edges);
    return 0;
}

